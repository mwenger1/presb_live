<h1>Listing models</h1>

<table>
  <thead>
    <tr>
      <th>Hangout</th>
      <th>Datetime</th>
      <th>Title</th>
      <th>Max participants</th>
      <th>Recordable</th>
      <th>Hangout url</th>
      <th>Description</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @models.each do |model| %>
      <tr>
        <td><%= model.Hangout %></td>
        <td><%= model.datetime %></td>
        <td><%= model.title %></td>
        <td><%= model.max_participants %></td>
        <td><%= model.recordable %></td>
        <td><%= model.hangout_url %></td>
        <td><%= model.description %></td>
        <td><%= model.user %></td>
        <td><%= link_to 'Show', model %></td>
        <td><%= link_to 'Edit', edit_model_path(model) %></td>
        <td><%= link_to 'Destroy', model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model', new_model_path %>
